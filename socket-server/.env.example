# Socket Server Environment Variables
# Copy this file to .env and fill in your actual values

# JWT Secret Key (MUST match the JWT_SECRET in Next.js .env.local)
JWT_SECRET=your-strong-secret-key-change-in-production

# Server Port
PORT=3001

# CORS Origin (allowed frontend URL)
# For local development: http://localhost:3000
# For production: https://your-vercel-app.vercel.app
CORS_ORIGIN=http://localhost:3000
